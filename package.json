{
  "name": "data-mocker-cli",
  "version": "1.0.1",
  "author": "Cristiam Sosa @Gunmer",
  "bin": {
    "dmcli": "./bin/run"
  },
  "bugs": "https://github.com/Gunmer/data-mocker-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.1.0",
    "cli-ux": "^5.4.9",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^5.2.7",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.17.24",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^8.0.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Gunmer/data-mocker-cli",
  "keywords": [
    "oclif",
    "generator",
    "mock",
    "fake"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dmcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Gunmer/data-mocker-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "coverage": "nyc mocha",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "description": "Cli to generate a test data set, with it you can generate any flat data structure, being able to specify the type of each field or attribute."
}
